{
	"name": "FinanceList-Desktop",
	"version": "1.1.2",
	"description": "A finance managing tool",
	"main": "./src/app/main.js",
	"scripts": {
		"start": "electron .",
		"test": "nyc mocha ./src/test/ --recursive",
		"package-mac": "electron-packager ./src/app --ignore=node_modules/wine-darwin --platform=darwin --arch=x64 --asar --icon=./img/tab.icns --prune=true --out=./application/",
		"create-dmg": "electron-installer-dmg ./application/FinanceList-Desktop-darwin-x64/FinanceList-Desktop.app FinanceList-Desktop-mac-x64",
		"package-win": "electron-packager ./src/app --ignore=node_modules/wine-darwin --platform=win32 --arch=x64 --asar --icon=./img/tab.ico --prune=true --out=./application/",
		"package-linux": "electron-packager ./src/app --ignore=node_modules/wine-darwin --platform=linux --arch=x64 --asar --icon=./img/tab.png --prune=true --out=./application/"
	},
	"repository": "https://github.com/Malte311/FinanceList-Desktop",
	"keywords": [
		"electron",
		"finance-management",
		"finance",
		"finances",
		"money-manager",
		"moneytracker",
		"money-spending"
	],
	"author": "Malte311",
	"license": "MIT",
	"devDependencies": {
		"electron": "^1.8.8",
		"electron-installer-dmg": "^0.2.1",
		"electron-packager": "^12.2.0",
		"jsdoc": "^3.6.3",
		"mocha": "^7.1.1",
		"npm": "^6.14.2",
		"nyc": "^15.0.0"
	},
	"dependencies": {
		"chart.js": "^2.9.3",
		"compare-versions": "^3.6.0",
		"electron-is-running-in-asar": "^1.0.0",
		"electron-json-storage": "^4.1.8",
		"jquery": "^3.4.1",
		"request": "^2.88.2"
	}
}
