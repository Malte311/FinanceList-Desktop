{
	"name": "FinanceList-Desktop",
	"version": "1.1.2",
	"description": "A finance managing tool",
	"main": "./src/app/main.js",
	"scripts": {
		"start": "electron . --dev",
		"test": "nyc mocha ./src/test/ --recursive --tmpdir",
		"package-mac": "electron-packager ./src/app --ignore=node_modules/wine-darwin --platform=darwin --arch=x64 --asar --icon=./src/app/img/tab.icns --prune=true --out=./application/",
		"create-dmg": "electron-installer-dmg ./application/FinanceList-Desktop-darwin-x64/FinanceList-Desktop.app FinanceList-Desktop-mac-x64",
		"package-win": "electron-packager ./src/app --ignore=node_modules/wine-darwin --platform=win32 --arch=x64 --asar --icon=./src/app/img/tab.ico --prune=true --out=./application/",
		"package-linux": "electron-packager ./src/app --ignore=node_modules/wine-darwin --platform=linux --arch=x64 --asar --icon=./src/app/img/tab.png --prune=true --out=./application/"
	},
	"nyc": {
		"exclude": "src/test/**/*.js"
	},
	"repository": "https://github.com/Malte311/FinanceList-Desktop",
	"keywords": [
		"electron",
		"finance-management",
		"finance",
		"finances",
		"money-manager",
		"moneytracker",
		"money-spending"
	],
	"author": "Malte311",
	"license": "MIT",
	"devDependencies": {
		"coveralls": "^3.1.0",
		"electron": "^10.1.3",
		"electron-installer-dmg": "^3.0.0",
		"electron-packager": "^15.1.0",
		"eslint": "^7.11.0",
		"jsdoc": "^3.6.3",
		"mocha": "^8.1.3",
		"npm": "^6.14.2",
		"nyc": "^15.0.0"
	},
	"dependencies": {
		"bootstrap": "^4.5.2",
		"bootstrap-datepicker": "^1.9.0",
		"chart.js": "^2.9.3",
		"compare-versions": "^3.6.0",
		"jquery": "^3.5.1",
		"jquery-ui-dist": "^1.12.1",
		"popper.js": "^1.16.1",
		"request": "^2.88.2"
	}
}
